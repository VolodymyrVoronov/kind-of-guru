generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id            Int      @id @default(autoincrement())
  firstName     String
  familyName    String
  information   String
  joinedCompany String
  home          Boolean  @default(true)
  office        Boolean  @default(false)
  intern        Boolean  @default(true)
  extern        Boolean  @default(false)
  roles         String
  level         String
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}

model Project {
  id          Int      @id @default(autoincrement())
  projectName String
  client      String
  information String
  priority    String
  level       String
  start       DateTime
  end         DateTime
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
